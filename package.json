{
  "name": "vuetify-markdown-editor",
  "version": "1.1.2",
  "description": "A Vue.js Component for editing and previewing markdown.",
  "main": "./dist/vuetify-markdown-editor.common.js",
  "files": [
    "dist/*",
    "src/components/*.vue",
    "src/util/*.js",
    "src/build-entry.js",
    "*.json",
    "screenshot/*"
  ],
  "bin": {
    "render-cli": "./src/util/render-cli.js"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name vuetify-markdown-editor --formats commonjs ./src/build-entry.js"
  },
  "author": "DCsunset",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DCsunset/vuetify-markdown-editor"
  },
  "keywords": [
    "vue",
    "vuetify",
    "markdown",
    "markdown-editor"
  ],
  "dependencies": {
    "@flowjs/flow.js": "^2.13.2",
    "emoji-mart-vue-fast": "^5.4.15",
    "highlight.js": "^9.15.10",
    "katex": "^0.10.2",
    "marked": "^0.7.0",
    "v-click-outside": "^2.1.3",
    "vue": "^2.6.6",
    "vuetify": "^2.0.15",
    "vuetify-loader": "^1.3.0"
  },
  "devDependencies": {
    "@mdi/font": "^4.4.95",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "css-loader": "^3.2.0",
    "deepmerge": "^4.0.0",
    "fibers": "^4.0.1",
    "sass": "^1.22.10",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  }
}
