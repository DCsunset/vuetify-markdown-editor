{
  "name": "vuetify-markdown-editor",
  "version": "1.5.0",
  "description": "A Vue.js Component for editing and previewing markdown.",
  "author": "DCsunset",
  "scripts": {
    "changelog": "conventional-changelog -p angular -r 0 > CHANGELOG.md",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name vuetify-markdown-editor --formats umd ./src/build-entry.js"
  },
  "main": "./dist/vuetify-markdown-editor.umd.js",
  "files": [
    "dist/*",
    "src/components/*.vue",
    "src/util/*.js",
    "src/build-entry.js",
    "*.json",
    "screenshot/*"
  ],
  "dependencies": {
    "@flowjs/flow.js": "^2.14.0",
    "core-js": "^3.4.4",
    "crypto-js": "^3.1.9-1",
    "emoji-mart-vue-fast": "^7.0.0",
    "highlight.js": "^9.18.1",
    "katex": "^0.11.1",
    "marked": "^0.8.0",
    "mermaid": "^8.4.7",
    "v-click-outside": "^3.0.1",
    "vue": "^2.6.11",
    "yargs": "^15.3.0"
  },
  "devDependencies": {
    "@mdi/font": "^4.8.95",
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "css-loader": "^3.4.2",
    "deepmerge": "^4.2.2",
    "fibers": "^4.0.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.9",
    "vuetify-loader": "1.x"
  },
  "peerDependencies": {
    "vuetify": "2.x"
  },
  "bin": {
    "render-cli": "./src/util/render-cli.js"
  },
  "keywords": [
    "vue",
    "vuetify",
    "markdown",
    "markdown-editor"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DCsunset/vuetify-markdown-editor"
  }
}
