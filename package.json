{
  "name": "vuetify-markdown-editor",
  "version": "2.0.2",
  "description": "A Vue.js Component for editing and previewing markdown.",
  "author": "DCsunset",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vuetify-markdown-editor --formats umd ./src/build-entry.js",
    "changelog": "conventional-changelog -p angular -r 0 > CHANGELOG.md"
  },
  "main": "./dist/vuetify-markdown-editor.umd.js",
  "files": [
    "dist/*",
    "src/components/*.vue",
    "src/util/*.js",
    "src/build-entry.js",
    "types/*",
    "*.json",
    "screenshot/*"
  ],
  "dependencies": {
    "@flowjs/flow.js": "^2.14.0",
    "core-js": "^3.4.4",
    "crypto-js": "^3.3.0",
    "emoji-mart-vue-fast": "^7.0.1",
    "highlight.js": "^9.18.1",
    "katex": "^0.11.1",
    "lodash": "^4.17.15",
    "marked": "^0.8.2",
    "mermaid": "^8.4.8",
    "v-click-outside": "^3.0.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@mdi/font": "^4.8.95",
    "@types/crypto-js": "^3.1.43",
    "@types/highlight.js": "^9.12.3",
    "@types/katex": "^0.11.0",
    "@types/lodash": "^4.14.149",
    "@types/marked": "^0.7.3",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-typescript": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "conventional-changelog-cli": "^2.0.31",
    "css-loader": "^3.4.2",
    "deepmerge": "^4.2.2",
    "fibers": "^4.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": "~3.7.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.19",
    "vuetify-loader": "1.x"
  },
  "peerDependencies": {
    "vuetify": "2.x"
  },
  "bin": {
    "render-cli": "./src/util/render-cli.js"
  },
  "keywords": [
    "vue",
    "vuetify",
    "markdown",
    "markdown-editor"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DCsunset/vuetify-markdown-editor"
  },
  "types": "./types/index.d.ts"
}
